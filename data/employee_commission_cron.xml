<odoo>
    <record id="ir_cron_auto_generate_commissions" model="ir.cron">
        <field name="name">Generate Employee Commissions</field>
        <field name="model_id" ref="model_employee_commission"/>
        <field name="state">code</field>
        <field name="code">model.generate_commissions()</field>
        <field name="interval_type">minutes</field>
        <field name="interval_number">1</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
